{
  "firebase": "blazing-inferno-9370",
  "headers": [ {
    "source" : "*",
    "headers" : [ {
      "key" : "Access-Control-Allow-Origin",
      "value" : "*"
    } ]
  } ],
  "public": "public",
  "ignore": [
    "firebase.json",
    "**/.*"
  ],
  "rewrites": [{
    "source": "**",
    "destination": "/index.html"
  }]
}